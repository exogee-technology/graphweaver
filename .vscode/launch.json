  "version": "0.2.0",
  "configurations": [
    {
      "name": "AWS Cognito Example",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["start"],
      "restart": true,
      "sourceMaps": true,
      "cwd": "${workspaceFolder}/src/examples/aws-cognito"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Admin UI in Chrome",
      "url": "http://localhost:9000",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "End to End - Auth",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["test-auth"],
      "restart": true,
      "sourceMaps": true,
      "cwd": "${workspaceFolder}/src/packages/end-to-end"
    },
    {
      "name": "End to End - Metadata",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["test-metadata"],
      "restart": true,
      "sourceMaps": true,
      "cwd": "${workspaceFolder}/src/packages/end-to-end"
    },
    {
      "name": "End to End - Start Server",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["start-server"],
      "restart": true,
      "sourceMaps": true,
      "cwd": "${workspaceFolder}/src/packages/end-to-end"
    },
    {
      "name": "Mailchimp Example",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["start"],
      "restart": true,
      "sourceMaps": true,
      "cwd": "${workspaceFolder}/src/examples/mailchimp"
    },
    {
      "name": "MySQL and PostgreSQL Database Example",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["start"],
      "restart": true,
      "sourceMaps": true,
      "cwd": "${workspaceFolder}/src/examples/databases"
    },
    {
      "name": "Rest Example",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["start"],
      "restart": true,
      "sourceMaps": true,
      "cwd": "${workspaceFolder}/src/examples/rest"
    },
    {
      "name": "SQLite Database Example",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["start"],
      "restart": true,
      "sourceMaps": true,
      "cwd": "${workspaceFolder}/src/examples/sqlite"
    },
    {
      "name": "Xero Example",
      "request": "launch",
      "cwd": "${workspaceFolder}/src/examples/xero",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["start"],
      "type": "node",
      "env": { "LOGGING_LEVEL": "trace" }
    }
  ]
}
