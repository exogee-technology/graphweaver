{
	"name": "@exogee/graphweaver-example-s3-storage",
	"version": "1.0.0",
	"license": "MIT",
	"private": true,
	"description": "Example of connecting Graphweaver to an AWS S3 bucket.",
	"main": "lib/index.js",
	"source": "src/index.ts",
