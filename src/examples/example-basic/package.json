{
	"name": "@exogee/graphweaver-example-basic",
	"version": "0.1.0-alpha.0",
	"license": "MIT",
	"description": "An example project for @exogee/graphweaver",
	"bin": "bin/index.js",
	"files": [
		"bin"
	],
	"scripts": {
		"build": "tsc && printf '%s\n%s\n' \"#!/usr/bin/env node\" \"$(cat bin/index.js)\" > bin/index.js",
		"start": "ts-node src/index.ts",
		"prettier": "prettier --write .",
		"test": "pnpm test:tsc && pnpm test:prettier",
		"test:tsc": "tsc --noEmit",
		"test:prettier": "prettier --check ."
	},
	"dependencies": {
		"@exogee/graphweaver-apollo": "workspace:0.1.0-alpha.1",
		"@exogee/graphweaver": "workspace:0.1.0-alpha.0",
		"@exogee/graphweaver-mikroorm": "workspace:0.1.0-alpha.1",
		"@mikro-orm/core": "5.4.2",
		"@mikro-orm/postgresql": "5.4.2",
		"apollo-server": "3.11.1",
		"apollo-server-core": "3.10.3",
		"dotenv": "16.0.0",
		"graphql": "15.8.0",
		"reflect-metadata": "0.1.13",
		"type-graphql": "1.1.1"
	},
	"devDependencies": {
		"@types/node": "14.14.10",
		"open": "8.4.0",
		"ts-node": "10.9.1",
		"typescript": "4.5.4"
	}
}
