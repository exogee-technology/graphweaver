{
	"name": "mikro-orm-sqlite-wasm",
	"version": "2.19.0",
	"description": "MikroORM SQLite Driver Wasm",
	"license": "Apache-2.0",
	"scripts": {
		"build": "npm run build:js && npm run build:types",
		"build:js": "node build.js",
		"build:types": "tsc --emitDeclarationOnly",
		"package:pack": "pnpm pack",
		"prettier": "prettier --write src/**/*.ts",
		"generate:schema": "tsx ./src/utils/generate-db-schema.ts",
		"version": "npm version --no-git-tag-version"
	},
	"main": "lib/index.js",
	"source": "src/index.ts",
	"directories": {
		"lib": "lib"
	},
	"files": [
		"lib"
	],
	"dependencies": {
		"@mikro-orm/knex": "6.4.16",
		"@mikro-orm/sqlite": "6.4.16",
		"node-sqlite3-wasm": "0.8.37"
	},
	"devDependencies": {
		"@types/node": "22.13.14",
		"esbuild": "0.25.1",
		"glob": "11.0.3",
		"tsx": "4.19.3",
		"typescript": "5.8.3"
	},
	"keywords": [
		"graphql",
		"gql",
		"headless",
		"cms",
		"postgres",
		"postgresql",
		"mysql",
		"sqlite"
	]
}
