{
	"name": "mikro-orm-sqlite-wasm",
	"version": "2.20.2",
	"description": "MikroORM SQLite Driver Wasm",
	"license": "Apache-2.0",
	"sideEffects": false,
	"scripts": {
		"build": "npm run build:js && npm run build:types",
		"build:js": "node build.js",
		"build:types": "tsc --emitDeclarationOnly",
		"package:pack": "pnpm pack",
		"prettier": "prettier --write src/**/*.ts",
		"generate:schema": "tsx ./src/utils/generate-db-schema.ts",
		"version": "npm version --no-git-tag-version"
	},
	"main": "lib/index.js",
	"source": "src/index.ts",
	"directories": {
		"lib": "lib"
	},
	"files": [
		"lib"
	],
	"dependencies": {
		"@mikro-orm/knex": "6.5.8",
		"@mikro-orm/sqlite": "6.5.8",
		"node-sqlite3-wasm": "0.8.50"
	},
	"devDependencies": {
		"@types/node": "24.7.2",
		"esbuild": "0.25.10",
		"glob": "11.0.3",
		"tsx": "4.20.6",
		"typescript": "5.9.3"
	},
	"keywords": [
		"graphql",
		"gql",
		"headless",
		"cms",
		"postgres",
		"postgresql",
		"mysql",
		"sqlite"
	]
}
