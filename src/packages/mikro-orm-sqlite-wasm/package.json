{
	"name": "mikro-orm-sqlite-wasm",
	"version": "2.0.0",
	"description": "MikroORM SQLite Driver Wasm",
	"license": "MIT",
	"scripts": {
		"build": "npm run build:js && npm run build:types",
		"build:js": "node build.js",
		"build:types": "tsc --emitDeclarationOnly",
		"package:pack": "pnpm pack",
		"prettier": "prettier --write src/**/*.ts",
		"generate:schema": "ts-node ./src/utils/generate-db-schema.ts",
		"version": "npm version --no-git-tag-version"
	},
	"main": "lib/index.js",
	"source": "src/index.ts",
	"directories": {
		"lib": "lib"
	},
	"files": [
		"lib"
	],
	"dependencies": {
		"@mikro-orm/knex": "6.2.9",
		"@mikro-orm/sqlite": "6.2.9",
		"node-sqlite3-wasm": "0.8.17"
	},
	"devDependencies": {
		"@types/node": "20.14.10",
		"esbuild": "0.23.0",
		"glob": "10.4.2",
		"typescript": "5.5.3"
	},
	"keywords": [
		"graphql",
		"gql",
		"headless",
		"cms",
		"postgres",
		"postgresql",
		"mysql",
		"sqlite"
	]
}
