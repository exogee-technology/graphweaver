{
	"name": "vite-plugin-graphweaver",
	"version": "0.2.24",
	"description": "A vite plugin for use with @exogee/graphweaver's admin UI",
	"license": "MIT",
	"main": "lib/index.js",
	"source": "src/index.ts",
	"directories": {
		"lib": "lib"
	},
	"files": [
		"lib"
	],
	"scripts": {
		"build": "npm run build:js && npm run build:types",
		"build:js": "node build.js",
		"build:types": "tsc --emitDeclarationOnly",
		"package:pack": "pnpm pack",
		"version": "npm version --no-git-tag-version"
	},
	"devDependencies": {
		"@types/node": "20.12.5",
		"esbuild": "0.15.5",
		"glob": "10.3.10",
		"typescript": "5.4.4",
		"vite": "5.2.6"
	},
	"peerDependencies": {
		"vite": "5"
	},
	"dependencies": {
		"@exogee/graphweaver-config": "workspace:*"
	},
	"keywords": [
		"graphql",
		"gql",
		"headless",
		"cms",
		"postgres",
		"postgresql",
		"mysql",
		"sqlite"
	]
}
