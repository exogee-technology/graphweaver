{
	"name": "@exogee/graphweaver-auth",
	"version": "1.0.0-beta.1",
	"description": "Row-Level Security support for @exogee/graphweaver",
	"license": "MIT",
	"scripts": {
		"build": "npm run build:js && npm run build:types",
		"build:js": "node build.js",
		"build:types": "tsc --emitDeclarationOnly",
		"build:watch": "pnpm build:js --watch",
		"package:pack": "pnpm pack",
		"prettier": "prettier --write src/**/*.ts",
		"codegen": "graphql-codegen --config ./codegen.yml",
		"version": "npm version --no-git-tag-version",
		"test": "vitest"
	},
	"main": "lib/index.js",
	"source": "src/index.ts",
	"directories": {
		"lib": "lib"
	},
	"files": [
		"lib"
	],
	"dependencies": {
		"@apollo/server": "4.10.2",
		"@exogee/graphweaver": "workspace:*",
		"@exogee/graphweaver-scalars": "workspace:*",
		"@exogee/logger": "workspace:*",
		"@simplewebauthn/server": "9.0.3",
		"apollo-server-errors": "3.3.1",
		"class-validator": "0.14.1",
		"dotenv": "16.4.5",
		"graphql": "16.8.1",
		"hash-wasm": "4.11.0",
		"jsonwebtoken": "9.0.2",
		"ms": "2.1.3",
		"otp-generator": "4.0.1",
		"web3-token": "1.0.6"
	},
	"devDependencies": {
		"@simplewebauthn/types": "9.0.1",
		"@types/jsonwebtoken": "9.0.6",
		"@types/ms": "0.7.34",
		"@types/node": "20.12.12",
		"@types/otp-generator": "4.0.2",
		"esbuild": "0.15.5",
		"glob": "10.3.12",
		"prettier": "3.2.5",
		"ts-node": "10.9.2",
		"typescript": "5.4.5",
		"vitest": "1.4.0"
	},
	"keywords": [
		"graphql",
		"gql",
		"headless",
		"cms",
		"postgres",
		"postgresql",
		"mysql",
		"sqlite"
	]
}
