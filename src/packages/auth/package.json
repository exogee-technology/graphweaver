{
	"name": "@exogee/graphweaver-auth",
	"version": "2.14.0",
	"description": "Row-Level Security support for @exogee/graphweaver",
	"license": "Apache-2.0",
	"scripts": {
		"build": "npm run build:js && npm run build:types",
		"build:js": "node build.js",
		"build:types": "tsc --emitDeclarationOnly",
		"build:watch": "pnpm build:js --watch",
		"package:pack": "pnpm pack",
		"prettier": "prettier --write src/**/*.ts",
		"codegen": "graphql-codegen --config ./codegen.yml",
		"version": "npm version --no-git-tag-version",
		"test": "vitest"
	},
	"main": "lib/index.js",
	"source": "src/index.ts",
	"directories": {
		"lib": "lib"
	},
	"files": [
		"lib"
	],
	"dependencies": {
		"@apollo/server": "4.11.3",
		"@exogee/graphweaver": "workspace:*",
		"@exogee/graphweaver-scalars": "workspace:*",
		"@exogee/graphweaver-server": "workspace:*",
		"@exogee/logger": "workspace:*",
		"@simplewebauthn/server": "13.1.1",
		"apollo-server-errors": "3.3.1",
		"class-validator": "0.14.1",
		"dotenv": "16.4.7",
		"graphql": "16.10.0",
		"hash-wasm": "4.12.0",
		"jsonwebtoken": "9.0.2",
		"jwks-rsa": "3.2.0",
		"ms": "2.1.3",
		"otp-generator": "4.0.1",
		"web3-token": "1.0.6"
	},
	"devDependencies": {
		"@simplewebauthn/types": "12.0.0",
		"@types/jsonwebtoken": "9.0.9",
		"@types/ms": "0.7.34",
		"@types/node": "22.13.14",
		"@types/otp-generator": "4.0.2",
		"esbuild": "0.25.1",
		"glob": "11.0.1",
		"prettier": "3.5.3",
		"typescript": "5.8.2",
		"vitest": "3.0.9"
	},
	"keywords": [
		"graphql",
		"gql",
		"headless",
		"cms",
		"postgres",
		"postgresql",
		"mysql",
		"sqlite"
	]
}
