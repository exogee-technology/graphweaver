{
	"name": "@exogee/graphweaver-auth",
	"version": "0.2.9",
	"description": "Row-Level Security support for @exogee/graphweaver",
	"license": "MIT",
	"scripts": {
		"build": "npm run build:js && npm run build:types",
		"build:js": "esbuild --outdir=lib --platform=node --format=cjs --sourcemap=linked `find src \\( -name '*.ts' \\) -not \\( -name '*.test.ts' \\)`",
		"build:types": "tsc --emitDeclarationOnly",
		"build:watch": "pnpm build:js --watch",
		"prettier": "prettier --write src/**/*.ts",
		"codegen": "graphql-codegen --config ./codegen.yml",
		"version": "npm version --no-git-tag-version",
		"test": "vitest"
	},
	"main": "lib/index.js",
	"source": "src/index.ts",
	"directories": {
		"lib": "lib"
	},
	"files": [
		"lib"
	],
	"dependencies": {
		"@apollo/server": "4.9.3",
		"@exogee/graphweaver": "workspace:*",
		"@exogee/graphweaver-scalars": "workspace:*",
		"@exogee/logger": "workspace:*",
		"@simplewebauthn/server": "8.2.0",
		"apollo-server-errors": "3.3.1",
		"class-validator": "0.14.0",
		"dotenv": "10.0.0",
		"graphql": "16.8.1",
		"jsonwebtoken": "9.0.0",
		"ms": "2.1.3",
		"otp-generator": "4.0.1",
		"type-graphql": "2.0.0-beta.2",
		"web3-token": "1.0.6",
		"argon2": "0.31.1"
	},
	"devDependencies": {
		"@simplewebauthn/typescript-types": "8.0.0",
		"@types/jsonwebtoken": "9.0.2",
		"@types/ms": "0.7.31",
		"@types/node": "20.2.5",
		"@types/otp-generator": "4.0.0",
		"esbuild": "0.15.5",
		"prettier": "2.8.8",
		"ts-node": "9.0.0",
		"tsc-watch": "4.2.9",
		"typescript": "5.0.2",
		"vitest": "0.34.5"
	},
	"keywords": [
		"graphql",
		"gql",
		"headless",
		"cms",
		"postgres",
		"postgresql",
		"mysql",
		"sqlite"
	]
}
