{
	"name": "@exogee/graphweaver-end-to-end",
	"version": "0.2.15",
	"description": "Graphweaver Test Suite",
	"license": "MIT",
	"scripts": {
		"test-build": ". ./scripts/test-build.sh",
		"import-database-sqlite": "tsx ./scripts/import-sqlite.ts",
		"import-database-postgres": ". ./scripts/import-postgres.sh",
		"import-database-mysql": ". ./scripts/import-mysql.sh",
		"seed-postgres:reseed": "export DATABASE_RESEED=true && node ./scripts/seed-postgres.js",
		"seed-postgres": "node ./scripts/seed-postgres.js",
		"start-server": "cd app && pnpm start",
		"start-auth-server": "cd ../../examples/rest && pnpm start",
		"dev-sqlite": "pnpm import-database-sqlite && pnpm start-server",
		"dev-postgres": "createdb gw && pnpm seed-postgres && pnpm import-database-postgres  && pnpm start-server",
		"dev-mysql": "pnpm import-database-mysql && pnpm start-server",
		"test-auth": "jest --runInBand --forceExit -- api/auth",
		"test-mysql": "export DATABASE=mysql && jest --runInBand -- api/mysql",
		"test-postgres": "export DATABASE=postgres && jest --runInBand -- api/postgres",
		"test-sqlite": "dotenv -e .env.sqlite -- jest --runInBand --forceExit ---- api/sqlite",
		"test-scalars": "jest --runInBand --forceExit ---- api/scalars",
		"test-aws-cognito": "jest --runInBand --forceExit ---- api/aws-cognito",
		"test-ui": "pnpm exec playwright test ./components ./issues",
		"test-ui:postgres": "export TEST_DATASOURCE=mikro-orm-postgres && pnpm test-ui --reporter line",
		"test-ui:init": "pnpm exec playwright install",
		"test-ui:report": "pnpm exec playwright show-report",
		"test-ui-auth": "pnpm exec playwright test ./auth",
		"version": "npm version --no-git-tag-version"
	},
	"dependencies": {
		"@exogee/graphweaver": "*",
		"@exogee/graphweaver-admin-ui-components": "workspace:*",
		"@exogee/graphweaver-auth": "*",
		"@exogee/graphweaver-aws": "*",
		"@exogee/graphweaver-mikroorm": "*",
		"@exogee/graphweaver-scalars": "*",
		"@exogee/graphweaver-server": "*",
		"@exogee/graphweaver-storage-provider": "workspace:*",
		"@mikro-orm/core": "5.7.14",
		"@mikro-orm/sqlite": "5.7.14",
		"@mikro-orm/postgresql": "5.7.14",
		"class-validator": "0.14.0",
		"dotenv-cli": "7.3.0",
		"ethers": "6.3.0",
		"graphql": "16.8.1",
		"graphweaver": "*",
		"reflect-metadata": "0.1.13",
		"type-graphql": "2.0.0-beta.2",
		"web3-token": "1.0.6"
	},
	"devDependencies": {
		"@playwright/test": "1.37.1",
		"@simplewebauthn/typescript-types": "8.0.0",
		"@types/jest": "26.0.23",
		"dotenv": "16.3.1",
		"graphql": "16.8.1",
		"graphql-tag": "2.12.6",
		"http-server": "14.1.1",
		"jest": "29.4.0",
		"mockdate": "3.0.5",
		"pg": "8.11.1",
		"serverless": "3.38.0",
		"serverless-offline": "13.3.2",
		"supertest-graphql": "1.1.4",
		"ts-jest": "29.0.5",
		"tsx": "4.7.0",
		"typescript": "5.0.2"
	},
	"keywords": [
		"graphql",
		"gql",
		"headless",
		"cms",
		"postgres",
		"postgresql",
		"mysql",
		"sqlite"
	]
}
