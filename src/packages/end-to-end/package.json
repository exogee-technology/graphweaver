{
	"name": "@exogee/graphweaver-end-to-end",
	"version": "1.0.0-beta.1",
	"description": "Graphweaver Test Suite",
	"license": "MIT",
	"scripts": {
		"import-auth": "pnpm unpack && tsx ./scripts/import-auth.ts",
		"import-storage-provider": "pnpm unpack && tsx ./scripts/import-storage-provider.ts",
		"import-database-sqlite": "pnpm unpack && tsx ./scripts/import-sqlite.ts",
		"import-database-postgres": "pnpm unpack && . ./scripts/import-postgres.sh",
		"import-database-mysql": "pnpm unpack && . ./scripts/import-mysql.sh",
		"seed-postgres": "node ./scripts/seed-postgres.js",
		"start-server": "cd app && pnpm start",
		"dev-sqlite": "pnpm import-database-sqlite && pnpm start-server",
		"dev-postgres": "createdb gw && pnpm seed-postgres && pnpm import-database-postgres  && pnpm start-server",
		"dev-mysql": "pnpm import-database-mysql && pnpm start-server",
		"unpack": "tsx ./scripts/unpack.ts",
		"test-auth": "jest --runInBand --forceExit -- api/auth",
		"test-metadata": "jest --runInBand --forceExit ---- api/metadata",
		"test-mysql": "export DATABASE=mysql && jest --runInBand -- api/mysql",
		"test-postgres": "export DATABASE=postgres && jest --runInBand -- api/postgres",
		"test-sqlite": "dotenv -e .env.sqlite -- jest --runInBand --forceExit ---- api/sqlite",
		"test-scalars": "jest --runInBand --forceExit ---- api/scalars",
		"test-ui-auth": "pnpm exec playwright test ./auth",
		"test-ui:init": "pnpm exec playwright install",
		"test-ui:report": "pnpm exec playwright show-report",
		"test-ui-storage-provider": "pnpm exec playwright test ./storage-provider",
		"test-ui-sqlite": "pnpm exec playwright test ./sqlite",
		"version": "npm version --no-git-tag-version"
	},
	"dependencies": {
		"@exogee/graphweaver": "workspace:*",
		"@exogee/graphweaver-admin-ui-components": "workspace:*",
		"@exogee/graphweaver-auth": "workspace:*",
		"@exogee/graphweaver-mikroorm": "workspace:*",
		"@exogee/graphweaver-scalars": "workspace:*",
		"@exogee/graphweaver-server": "workspace:*",
		"@exogee/graphweaver-storage-provider": "workspace:*",
		"@mikro-orm/core": "6.2.8",
		"@mikro-orm/postgresql": "6.2.8",
		"@mikro-orm/sqlite": "6.2.8",
		"class-validator": "0.14.1",
		"dotenv-cli": "7.4.2",
		"ethers": "6.3.0",
		"graphql": "16.8.1",
		"graphweaver": "*",
		"promisify-child-process": "4.1.2",
		"web3-token": "1.0.6"
	},
	"devDependencies": {
		"@playwright/test": "1.44.1",
		"@simplewebauthn/types": "9.0.1",
		"@types/jest": "29.5.12",
		"dotenv": "16.4.5",
		"graphql": "16.8.1",
		"graphql-tag": "2.12.6",
		"http-server": "14.1.1",
		"jest": "29.7.0",
		"mockdate": "3.0.5",
		"pg": "8.11.5",
		"supertest-graphql": "1.1.4",
		"ts-jest": "29.1.4",
		"tsx": "4.11.0",
		"typescript": "5.4.5"
	},
	"keywords": [
		"graphql",
		"gql",
		"headless",
		"cms",
		"postgres",
		"postgresql",
		"mysql",
		"sqlite"
	]
}
