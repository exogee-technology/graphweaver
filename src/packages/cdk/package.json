{
	"name": "@exogee/graphweaver-cdk",
	"version": "2.7.1",
	"description": "Deploy Graphweaver to AWS",
	"license": "Apache-2.0",
	"directories": {
		"lib": "lib"
	},
	"files": [
		"lib"
	],
	"main": "lib/index.js",
	"source": "src/index.ts",
	"scripts": {
		"cdk-bootstrap": "dotenv -- cdk bootstrap -v",
		"cdk-deploy": "dotenv -- cdk deploy --all",
		"cdk-synth": "dotenv -- cdk synth --verbose",
		"build": "npm run build:js && npm run build:types",
		"build:js": "node build.js",
		"build:types": "tsc --emitDeclarationOnly",
		"package:pack": "pnpm pack",
		"test": "dotenv -- vitest",
		"version": "npm version --no-git-tag-version",
		"build:docker": "docker build -f src/docker/Dockerfile ../../examples/s3-storage"
	},
	"dependencies": {
		"aws-cdk-lib": "2.164.1",
		"constructs": "10.3.0"
	},
	"devDependencies": {
		"@digitak/esrun": "3.2.26",
		"dotenv-cli": "7.4.2",
		"aws-cdk": "2.164.1",
		"@exogee/graphweaver": "workspace:*",
		"@types/node": "22.7.5",
		"esbuild": "0.24.0",
		"glob": "10.4.3",
		"typescript": "5.6.3",
		"vitest": "2.1.2"
	},
	"keywords": [
		"graphql",
		"gql",
		"headless",
		"cms",
		"postgres",
		"postgresql",
		"mysql",
		"sqlite"
	]
}
