{
	"name": "@exogee/graphweaver-cdk",
	"version": "2.20.2",
	"description": "Deploy Graphweaver to AWS",
	"license": "Apache-2.0",
	"sideEffects": false,
	"directories": {
		"lib": "lib"
	},
	"files": [
		"lib"
	],
	"main": "lib/index.js",
	"source": "src/index.ts",
	"scripts": {
		"cdk-bootstrap": "dotenv -- cdk bootstrap -v",
		"cdk-deploy": "dotenv -- cdk deploy --all",
		"cdk-synth": "dotenv -- cdk synth --verbose",
		"build": "npm run build:js && npm run build:types",
		"build:js": "node build.js",
		"build:types": "tsc --emitDeclarationOnly",
		"package:pack": "pnpm pack",
		"test": "dotenv -- vitest",
		"version": "npm version --no-git-tag-version",
		"build:docker": "docker build -f src/docker/Dockerfile ../../examples/s3-storage"
	},
	"peerDependencies": {
		"aws-cdk-lib": "^2.173.1",
		"constructs": "^10.4.2"
	},
	"devDependencies": {
		"@digitak/esrun": "3.2.26",
		"dotenv-cli": "10.0.0",
		"aws-cdk": "2.1030.0",
		"aws-cdk-lib": "2.219.0",
		"constructs": "10.4.2",
		"@exogee/graphweaver": "workspace:*",
		"@types/node": "24.7.0",
		"esbuild": "0.25.10",
		"glob": "11.0.3",
		"typescript": "5.9.2",
		"vitest": "3.2.4"
	},
	"keywords": [
		"graphql",
		"gql",
		"headless",
		"cms",
		"postgres",
		"postgresql",
		"mysql",
		"sqlite"
	]
}
