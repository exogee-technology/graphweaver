{
	"name": "@exogee/graphweaver-auth-ui-components",
	"version": "1.0.0-beta.1",
	"description": "Components from Graphweaver's Auth UI which you can use in your projects as you like",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"build": "npm run build:js && npm run build:types",
		"build:js": "node build.js",
		"build:types": "tsc --emitDeclarationOnly",
		"build:watch": "pnpm build:js --watch",
		"package:pack": "pnpm pack",
		"prettier": "prettier --write .",
		"codegen": "graphql-codegen --config ./codegen.yml",
		"test:tsc": "tsc --noEmit",
		"test:prettier": "prettier --check .",
		"version": "npm version --no-git-tag-version"
	},
	"main": "lib/index.js",
	"source": "src/index.ts",
	"directories": {
		"lib": "lib"
	},
	"files": [
		"lib"
	],
	"dependencies": {
		"@apollo/client": "3.10.4",
		"@simplewebauthn/browser": "9.0.1",
		"@usedapp/core": "1.2.16",
		"ethers": "5.7.2",
		"web3-token": "1.0.6"
	},
	"peerDependencies": {
		"@exogee/graphweaver-admin-ui-components": "workspace:*",
		"formik": "2.2.9",
		"graphql": "16.8.1",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-router-dom": "6.23.1"
	},
	"devDependencies": {
		"@exogee/graphweaver-admin-ui-components": "workspace:*",
		"@types/react": "18.3.3",
		"@types/react-dom": "18.3.0",
		"esbuild": "0.20.2",
		"esbuild-plugin-svgr": "2.1.0",
		"glob": "10.4.1",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"typescript": "5.4.5",
		"vite": "5.2.11",
		"vite-plugin-svgr": "4.2.0",
		"@simplewebauthn/types": "9.0.1"
	},
	"keywords": [
		"graphql",
		"gql",
		"headless",
		"cms",
		"postgres",
		"postgresql",
		"mysql",
		"sqlite"
	]
}
