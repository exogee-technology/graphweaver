{
	"name": "@exogee/graphweaver-auth-ui-components",
	"version": "2.21.3",
	"description": "Components from Graphweaver's Auth UI which you can use in your projects as you like",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/exogee-technology/graphweaver.git"
	},
	"type": "module",
	"sideEffects": false,
	"scripts": {
		"build": "npm run build:js && npm run build:types",
		"build:js": "node build.js",
		"build:types": "tsc --emitDeclarationOnly",
		"build:watch": "pnpm build:js --watch",
		"package:pack": "pnpm pack",
		"prettier": "prettier --write .",
		"codegen": "graphql-codegen --config ./codegen.yml",
		"test:tsc": "tsc --noEmit",
		"test:prettier": "prettier --check .",
		"version": "npm version --no-git-tag-version"
	},
	"main": "lib/index.js",
	"source": "src/index.ts",
	"directories": {
		"lib": "lib"
	},
	"files": [
		"lib"
	],
	"dependencies": {
		"@apollo/client": "3.13.5",
		"@simplewebauthn/browser": "13.2.2",
		"@usedapp/core": "1.2.16",
		"ethers": "5.8.0",
		"web3-token": "1.0.6",
		"wouter": "3.9.0"
	},
	"peerDependencies": {
		"@auth0/auth0-spa-js": "^2.1.3",
		"@azure/msal-browser": "^3.25.0",
		"@azure/msal-react": "^3.0.0",
		"@exogee/graphweaver-admin-ui-components": "workspace:*",
		"@okta/okta-auth-js": "^7.9.0",
		"formik": "^2.2.9",
		"graphql": "16",
		"react": "^18.0.0 || ^19.0.0",
		"react-dom": "^18.0.0 || ^19.0.0"
	},
	"devDependencies": {
		"@auth0/auth0-spa-js": "2.16.0",
		"@azure/msal-browser": "4.25.1",
		"@azure/msal-react": "3.0.22",
		"@exogee/graphweaver-admin-ui-components": "workspace:*",
		"@okta/okta-auth-js": "7.14.0",
		"@simplewebauthn/types": "12.0.0",
		"@types/react": "19.2.13",
		"@types/react-dom": "19.2.3",
		"esbuild": "0.27.1",
		"glob": "11.1.0",
		"react": "19.2.4",
		"react-dom": "19.2.4",
		"typescript": "5.9.3",
		"vite": "7.3.0"
	},
	"keywords": [
		"graphql",
		"gql",
		"headless",
		"cms",
		"postgres",
		"postgresql",
		"mysql",
		"sqlite"
	]
}
