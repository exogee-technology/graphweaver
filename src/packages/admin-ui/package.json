{
	"name": "@exogee/graphweaver-admin-ui",
	"private": true,
	"version": "0.1.0-alpha.6",
	"type": "module",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"scripts": {
		"build": "npm run build:js && npm run build:types",
		"build:js": "esbuild --outdir=lib --platform=node --format=cjs `find src \\( -name '*.ts' \\)`",
		"build:types": "tsc --noEmit false --emitDeclarationOnly --declaration --outDir lib",
		"prettier": "prettier --write .",
		"test:prettier": "prettier --check .",
		"test:tsc": "tsc --noEmit"
	},
	"dependencies": {
		"@apollo/client": "3.7.1",
		"@exogee/graphweaver-admin-ui-components": "workspace:0.1.0-alpha.5",
		"classnames": "2.3.2",
		"graphql": "16.6.0",
		"pluralize": "8.0.0",
		"prop-types": "15.8.1",
		"react": "18.2.0",
		"react-data-grid": "7.0.0-beta.20",
		"react-dom": "18.2.0",
		"react-modal": "3.16.1",
		"react-router-dom": "6.4.3",
		"vite-plugin-svgr": "2.2.2"
	},
	"devDependencies": {
		"@types/pluralize": "0.0.29",
		"@types/react": "18.0.25",
		"@types/react-dom": "18.0.9",
		"@types/react-modal": "3.13.1",
		"@vitejs/plugin-react": "2.2.0",
		"esbuild": "0.15.5",
		"typescript": "4.9.4",
		"vite": "3.2.4"
	}
}
