{
	"name": "@exogee/graphweaver-builder",
	"version": "2.20.1",
	"description": "A tool for building and running Graphweaver projects",
	"license": "Apache-2.0",
	"sideEffects": false,
	"scripts": {
		"build": "npm run build:js && npm run build:types",
		"build:js": "node build.js",
		"build:types": "tsc --emitDeclarationOnly",
		"package:pack": "pnpm pack",
		"prettier": "prettier --write .",
		"postinstall": "node build.js",
		"version": "npm version --no-git-tag-version",
		"test": "vitest"
	},
	"main": "lib/index.js",
	"source": "src/index.ts",
	"dependencies": {
		"@exogee/graphweaver-admin-ui": "workspace:*",
		"@exogee/graphweaver-config": "workspace:*",
		"@exogee/graphweaver-mikroorm": "workspace:*",
		"@graphql-codegen/add": "6.0.0",
		"@graphql-codegen/cli": "5.0.7",
		"@graphql-codegen/near-operation-file-preset": "3.1.0",
		"@graphql-codegen/typescript": "5.0.0",
		"@graphql-codegen/typescript-operations": "5.0.0",
		"@graphql-codegen/typescript-react-apollo": "4.3.3",
		"@serverless/utils": "6.15.0",
		"@vitejs/plugin-react": "4.3.4",
		"class-validator": "0.14.2",
		"dotenv": "17.2.2",
		"esbuild": "0.25.9",
		"esbuild-css-modules-plugin": "3.1.5",
		"glob": "11.0.3",
		"graphql": "16.11.0",
		"graphql-tag": "2.12.6",
		"hash-wasm": "4.12.0",
		"log-node": "8.0.3",
		"omgopass": "3.2.1",
		"rimraf": "5.0.8",
		"rollup-plugin-visualizer": "6.0.3",
		"serverless-offline": "14.4.0",
		"vite": "7.1.5",
		"vite-plugin-graphweaver": "workspace:*"
	},
	"devDependencies": {
		"@types/jsonwebtoken": "9.0.10",
		"@types/node": "24.3.1",
		"@types/omgopass": "3.2.3",
		"jsonwebtoken": "9.0.2",
		"prettier": "3.6.2",
		"typescript": "5.9.2",
		"vitest": "3.2.4"
	},
	"keywords": [
		"graphql",
		"gql",
		"headless",
		"cms",
		"postgres",
		"postgresql",
		"mysql",
		"sqlite"
	]
}
