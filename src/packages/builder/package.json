{
	"name": "@exogee/graphweaver-builder",
	"version": "2.0.0",
	"description": "A tool for building and running Graphweaver projects",
	"license": "MIT",
	"scripts": {
		"build": "npm run build:js && npm run build:types",
		"build:js": "node build.js",
		"build:types": "tsc --emitDeclarationOnly",
		"package:pack": "pnpm pack",
		"prettier": "prettier --write .",
		"postinstall": "node build.js",
		"version": "npm version --no-git-tag-version"
	},
	"main": "lib/index.js",
	"source": "src/index.ts",
	"dependencies": {
		"@exogee/graphweaver-config": "workspace:*",
		"@graphql-codegen/add": "5.0.3",
		"@graphql-codegen/cli": "5.0.2",
		"@graphql-codegen/near-operation-file-preset": "3.0.0",
		"@graphql-codegen/typescript": "4.0.9",
		"@graphql-codegen/typescript-operations": "4.2.3",
		"@graphql-codegen/typescript-react-apollo": "4.3.0",
		"@serverless/utils": "6.15.0",
		"@vitejs/plugin-react": "4.3.1",
		"class-validator": "0.14.1",
		"dotenv": "16.4.5",
		"esbuild": "0.23.0",
		"esbuild-css-modules-plugin": "3.1.2",
		"glob": "10.4.2",
		"graphql": "16.9.0",
		"graphql-tag": "2.12.6",
		"log-node": "8.0.3",
		"rimraf": "5.0.8",
		"rollup-plugin-visualizer": "5.12.0",
		"serverless-offline": "13.6.0",
		"vite": "5.3.3",
		"vite-plugin-graphweaver": "workspace:*"
	},
	"devDependencies": {
		"@types/node": "20.14.10",
		"prettier": "3.3.2",
		"typescript": "5.5.3"
	},
	"keywords": [
		"graphql",
		"gql",
		"headless",
		"cms",
		"postgres",
		"postgresql",
		"mysql",
		"sqlite"
	]
}
