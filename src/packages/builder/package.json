{
	"name": "@exogee/graphweaver-builder",
	"version": "1.0.0-beta.1",
	"description": "A tool for building and running Graphweaver projects",
	"license": "MIT",
	"scripts": {
		"build": "npm run build:js && npm run build:types",
		"build:js": "node build.js",
		"build:types": "tsc --emitDeclarationOnly",
		"package:pack": "pnpm pack",
		"prettier": "prettier --write .",
		"postinstall": "node build.js",
		"version": "npm version --no-git-tag-version"
	},
	"main": "lib/index.js",
	"source": "src/index.ts",
	"dependencies": {
		"@exogee/graphweaver-config": "workspace:*",
		"@graphql-codegen/add": "5.0.2",
		"@graphql-codegen/cli": "5.0.2",
		"@graphql-codegen/typescript": "4.0.6",
		"@graphql-codegen/typescript-operations": "4.2.0",
		"@graphql-codegen/near-operation-file-preset": "3.0.0",
		"@graphql-codegen/typescript-react-apollo": "4.3.0",
		"@serverless/utils": "6.15.0",
		"@vitejs/plugin-react": "4.2.1",
		"class-validator": "0.14.1",
		"dotenv": "16.4.5",
		"esbuild": "0.15.5",
		"esbuild-css-modules-plugin": "2.7.1",
		"glob": "10.3.12",
		"graphql": "16.8.1",
		"graphql-tag": "2.12.6",
		"rimraf": "5.0.5",
		"rollup-plugin-visualizer": "5.12.0",
		"serverless": "3.38.0",
		"serverless-dotenv-plugin": "6.0.0",
		"serverless-offline": "13.3.3",
		"vite": "5.2.8",
		"vite-plugin-graphweaver": "workspace:*",
		"vite-plugin-svgr": "3.2.0"
	},
	"devDependencies": {
		"@types/node": "20.12.7",
		"@types/rimraf": "4.0.5",
		"prettier": "3.2.5"
	},
	"keywords": [
		"graphql",
		"gql",
		"headless",
		"cms",
		"postgres",
		"postgresql",
		"mysql",
		"sqlite"
	]
}
