{
	"name": "@exogee/graphweaver-builder",
	"version": "0.2.9",
	"description": "A tool for building and running Graphweaver projects",
	"license": "MIT",
	"scripts": {
		"build": "npm run build:js && npm run build:types",
		"build:js": "node build.js",
		"build:types": "tsc --emitDeclarationOnly",
		"prettier": "prettier --write .",
		"postinstall": "node build.js",
		"version": "npm version --no-git-tag-version"
	},
	"main": "lib/index.js",
	"source": "src/index.ts",
	"dependencies": {
		"@exogee/graphweaver-admin-ui": "workspace:*",
		"@exogee/graphweaver-config": "workspace:*",
		"@graphql-codegen/add": "5.0.0",
		"@graphql-codegen/cli": "4.0.1",
		"@graphql-codegen/typescript": "4.0.1",
		"@graphql-codegen/typescript-operations": "4.0.1",
		"@graphql-codegen/near-operation-file-preset": "2.5.0",
		"@graphql-codegen/typescript-react-apollo": "3.3.7",
		"@serverless/utils": "6.8.2",
		"@vitejs/plugin-react": "3.1.0",
		"class-validator": "0.14.0",
		"cross-fetch": "3.1.6",
		"dotenv": "16.0.3",
		"esbuild": "0.15.5",
		"esbuild-css-modules-plugin": "2.7.1",
		"graphql": "16.8.1",
		"graphql-tag": "2.12.6",
		"rimraf": "3.0.2",
		"rollup-plugin-visualizer": "5.8.3",
		"serverless": "3.34.0",
		"serverless-dotenv-plugin": "4.0.2",
		"serverless-offline": "12.0.2",
		"type-graphql": "2.0.0-beta.2",
		"vite": "4.3.9",
		"vite-plugin-graphweaver": "workspace:*",
		"vite-plugin-svgr": "3.2.0"
	},
	"devDependencies": {
		"@types/node": "20.2.5",
		"@types/rimraf": "3.0.2",
		"prettier": "2.8.8"
	},
	"keywords": [
		"graphql",
		"gql",
		"headless",
		"cms",
		"postgres",
		"postgresql",
		"mysql",
		"sqlite"
	]
}
