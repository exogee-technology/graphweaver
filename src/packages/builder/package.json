{
	"name": "@exogee/graphweaver-builder",
	"version": "2.12.3",
	"description": "A tool for building and running Graphweaver projects",
	"license": "Apache-2.0",
	"scripts": {
		"build": "npm run build:js && npm run build:types",
		"build:js": "node build.js",
		"build:types": "tsc --emitDeclarationOnly",
		"package:pack": "pnpm pack",
		"prettier": "prettier --write .",
		"postinstall": "node build.js",
		"version": "npm version --no-git-tag-version",
		"test": "vitest"
	},
	"main": "lib/index.js",
	"source": "src/index.ts",
	"dependencies": {
		"@exogee/graphweaver-admin-ui": "workspace:*",
		"@exogee/graphweaver-config": "workspace:*",
		"@exogee/graphweaver-mikroorm": "workspace:*",
		"@graphql-codegen/add": "5.0.3",
		"@graphql-codegen/cli": "5.0.5",
		"@graphql-codegen/near-operation-file-preset": "3.0.0",
		"@graphql-codegen/typescript": "4.1.4",
		"@graphql-codegen/typescript-operations": "4.4.0",
		"@graphql-codegen/typescript-react-apollo": "4.3.2",
		"@serverless/utils": "6.15.0",
		"@vitejs/plugin-react": "4.3.4",
		"class-validator": "0.14.1",
		"dotenv": "16.4.7",
		"esbuild": "0.25.0",
		"esbuild-css-modules-plugin": "3.1.4",
		"glob": "11.0.1",
		"graphql": "16.10.0",
		"graphql-tag": "2.12.6",
		"hash-wasm": "4.12.0",
		"log-node": "8.0.3",
		"omgopass": "3.2.1",
		"rimraf": "5.0.8",
		"rollup-plugin-visualizer": "5.14.0",
		"serverless-offline": "14.4.0",
		"vite": "6.1.0",
		"vite-plugin-graphweaver": "workspace:*"
	},
	"devDependencies": {
		"@types/jsonwebtoken": "9.0.9",
		"@types/node": "22.13.10",
		"@types/omgopass": "3.2.3",
		"jsonwebtoken": "9.0.2",
		"prettier": "3.4.2",
		"typescript": "5.7.2",
		"vitest": "3.0.7"
	},
	"keywords": [
		"graphql",
		"gql",
		"headless",
		"cms",
		"postgres",
		"postgresql",
		"mysql",
		"sqlite"
	]
}
