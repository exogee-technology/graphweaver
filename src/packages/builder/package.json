{
	"name": "@exogee/graphweaver-builder",
	"version": "0.1.0",
	"description": "A tool for building and running GraphWeaver projects",
	"license": "MIT",
	"scripts": {
		"build": "npm run build:js && npm run build:types",
		"build:js": "node build.js",
		"build:types": "tsc --emitDeclarationOnly",
		"prettier": "prettier --write .",
		"postinstall": "node build.js",
		"version": "npm version --no-git-tag-version"
	},
	"main": "lib/index.js",
	"source": "src/index.ts",
	"dependencies": {
		"@exogee/graphweaver-admin-ui": "workspace:0.1.0-alpha.5",
		"@serverless/utils": "6.8.2",
		"@vitejs/plugin-react": "2.2.0",
		"dotenv": "16.0.3",
		"esbuild": "0.15.5",
		"esbuild-css-modules-plugin": "2.3.1",
		"rimraf": "3.0.2",
		"rollup-plugin-visualizer": "5.8.3",
		"serverless": "3.25.1",
		"serverless-dotenv-plugin": "4.0.2",
		"serverless-offline": "12.0.2",
		"vite": "3.2.4",
		"vite-plugin-graphweaver": "workspace:0.1.0-alpha.5",
		"vite-plugin-svgr": "2.2.2"
	},
	"devDependencies": {
		"@types/node": "18.11.9",
		"@types/rimraf": "3.0.2",
		"prettier": "2.7.1"
	}
}
