{
	"name": "@exogee/graphweaver-admin-ui-components",
	"version": "0.1.1",
	"description": "Components from Graphweaver's admin UI which you can use in your projects as you like",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"build": "npm run build:js && npm run build:types",
		"build:js": "node build.js",
		"build:types": "tsc --emitDeclarationOnly",
		"build:watch": "pnpm build:js --watch",
		"prettier": "prettier --write .",
		"codegen": "graphql-codegen --config ./codegen.yml",
		"test": "pnpm test:unit",
		"test:unit": "jest --passWithNoTests",
		"test:tsc": "tsc --noEmit",
		"test:prettier": "prettier --check .",
		"version": "npm version --no-git-tag-version"
	},
	"main": "lib/index.js",
	"source": "src/index.ts",
	"directories": {
		"lib": "lib"
	},
	"files": [
		"lib"
	],
	"dependencies": {
		"@apollo/client": "3.7.1",
		"@graphiql/toolkit": "0.8.2",
		"classnames": "2.3.2",
		"formik": "2.2.9",
		"graphiql": "2.4.0",
		"graphql": "16.6.0",
		"pluralize": "8.0.0",
		"react-data-grid": "7.0.0-beta.20"
	},
	"peerDependencies": {
		"graphql": "16.6.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-router-dom": "6.4.3"
	},
	"devDependencies": {
		"@types/pluralize": "0.0.29",
		"@types/react": "18.0.25",
		"esbuild": "0.15.5",
		"esbuild-css-modules-plugin": "2.7.1",
		"esbuild-plugin-svgr": "1.0.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-router-dom": "6.4.3",
		"typescript": "4.9.4",
		"vite": "3.2.4",
		"vite-plugin-svgr": "2.2.2"
	}
}
