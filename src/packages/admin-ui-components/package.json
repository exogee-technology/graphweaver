{
	"name": "@exogee/graphweaver-admin-ui-components",
	"version": "2.12.2",
	"description": "Components from Graphweaver's admin UI which you can use in your projects as you like",
	"license": "Apache-2.0",
	"type": "module",
	"scripts": {
		"build": "npm run build:js && npm run build:types",
		"build:js": "node build.js",
		"build:types": "tsc --emitDeclarationOnly",
		"package:pack": "pnpm pack",
		"build:watch": "pnpm build:js --watch",
		"prettier": "prettier --write .",
		"codegen": "graphql-codegen --config ./codegen.yml",
		"test:tsc": "tsc --noEmit",
		"test:prettier": "prettier --check .",
		"version": "npm version --no-git-tag-version"
	},
	"main": "lib/index.js",
	"source": "src/index.ts",
	"directories": {
		"lib": "lib"
	},
	"files": [
		"lib"
	],
	"dependencies": {
		"@apollo/client": "3.13.1",
		"@exogee/graphweaver-apollo-client": "workspace:*",
		"@graphiql/toolkit": "0.11.1",
		"@tanstack/react-table": "8.20.6",
		"@tiptap/extension-link": "2.11.5",
		"@tiptap/react": "2.11.5",
		"@tiptap/starter-kit": "2.11.5",
		"clsx": "2.1.1",
		"downshift": "9.0.8",
		"graphiql": "3.8.3",
		"graphql-deduplicator": "2.0.6",
		"luxon": "3.5.0",
		"papaparse": "5.5.2",
		"react-day-picker": "9.5.1",
		"react-error-boundary": "5.0.0",
		"react-hot-toast": "2.4.1",
		"react-resizable-panels": "2.1.7",
		"react-syntax-highlighter": "15.6.1",
		"tiptap-markdown": "0.8.10",
		"wouter": "3.6.0"
	},
	"peerDependencies": {
		"formik": "^2.4.6",
		"graphql": "16",
		"react": "^18.0.0 || ^19.0.0",
		"react-dom": "^18.0.0 || ^19.0.0"
	},
	"devDependencies": {
		"@types/graphql-deduplicator": "2.0.2",
		"@types/luxon": "3.4.2",
		"@types/papaparse": "5.3.15",
		"@types/react": "19.0.10",
		"@types/react-syntax-highlighter": "15.5.13",
		"esbuild": "0.25.0",
		"esbuild-plugin-copy": "2.1.1",
		"formik": "2.4.6",
		"glob": "11.0.1",
		"graphql": "16.10.0",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"typescript": "5.7.2",
		"vite": "6.1.0"
	},
	"keywords": [
		"graphql",
		"gql",
		"headless",
		"cms",
		"postgres",
		"postgresql",
		"mysql",
		"sqlite"
	]
}
