{
	"name": "@exogee/graphweaver-admin-ui-components",
	"version": "0.2.9",
	"description": "Components from Graphweaver's admin UI which you can use in your projects as you like",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"build": "npm run build:js && npm run build:types",
		"build:js": "node build.js",
		"build:types": "tsc --emitDeclarationOnly",
		"build:watch": "pnpm build:js --watch",
		"prettier": "prettier --write .",
		"codegen": "graphql-codegen --config ./codegen.yml",
		"test:tsc": "tsc --noEmit",
		"test:prettier": "prettier --check .",
		"version": "npm version --no-git-tag-version"
	},
	"main": "lib/index.js",
	"source": "src/index.ts",
	"directories": {
		"lib": "lib"
	},
	"files": [
		"lib"
	],
	"dependencies": {
		"@apollo/client": "3.7.1",
		"@graphiql/toolkit": "0.8.2",
		"classnames": "2.3.2",
		"formik": "2.2.9",
		"graphiql": "2.4.5",
		"graphql": "16.8.1",
		"graphql-deduplicator": "2.0.6",
		"luxon": "2.5.2",
		"pluralize": "8.0.0",
		"react-data-grid": "7.0.0-beta.20",
		"react-hot-toast": "2.4.1"
	},
	"peerDependencies": {
		"graphql": "16.8.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-router-dom": "6.4.3"
	},
	"devDependencies": {
		"@types/graphql-deduplicator": "2.0.0",
		"@types/luxon": "2.0.7",
		"@types/pluralize": "0.0.29",
		"@types/react": "18.0.25",
		"esbuild": "0.15.5",
		"esbuild-css-modules-plugin": "2.7.1",
		"esbuild-plugin-svgr": "1.0.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-router-dom": "6.4.3",
		"typescript": "5.0.2",
		"vite": "4.3.9",
		"vite-plugin-svgr": "3.2.0"
	},
	"keywords": [
		"graphql",
		"gql",
		"headless",
		"cms",
		"postgres",
		"postgresql",
		"mysql",
		"sqlite"
	]
}
