{
	"name": "@exogee/graphweaver-admin-ui-components",
	"version": "2.19.9",
	"description": "Components from Graphweaver's admin UI which you can use in your projects as you like",
	"license": "Apache-2.0",
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "npm run build:js && npm run build:types",
		"build:js": "node build.js",
		"build:types": "tsc --emitDeclarationOnly",
		"package:pack": "pnpm pack",
		"build:watch": "pnpm build:js --watch",
		"prettier": "prettier --write .",
		"codegen": "graphql-codegen --config ./codegen.yml",
		"test:tsc": "tsc --noEmit",
		"test:prettier": "prettier --check .",
		"version": "npm version --no-git-tag-version",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"source": "src/index.ts",
	"directories": {
		"lib": "lib"
	},
	"files": [
		"lib"
	],
	"dependencies": {
		"@apollo/client": "3.13.5",
		"@exogee/graphweaver-apollo-client": "workspace:*",
		"@floating-ui/dom": "1.7.3",
		"@graphiql/toolkit": "0.11.3",
		"@tanstack/react-form": "1.19.3",
		"@tanstack/react-table": "8.21.3",
		"@tiptap/extension-link": "3.4.0",
		"@tiptap/react": "3.4.0",
		"@tiptap/starter-kit": "3.4.0",
		"@types/react-dom": "19.1.7",
		"clsx": "2.1.1",
		"downshift": "9.0.10",
		"graphiql": "4.1.2",
		"graphql-deduplicator": "2.0.6",
		"luxon": "3.7.1",
		"motion": "12.23.12",
		"papaparse": "5.5.3",
		"react-day-picker": "9.9.0",
		"react-error-boundary": "6.0.0",
		"react-fast-compare": "3.2.2",
		"react-hot-toast": "2.6.0",
		"react-resizable-panels": "3.0.5",
		"react-syntax-highlighter": "15.6.6",
		"tiptap-markdown": "0.8.10",
		"wouter": "3.7.1",
		"zod": "4.1.5"
	},
	"peerDependencies": {
		"formik": "^2.4.6",
		"graphql": "16",
		"react": "^18.0.0 || ^19.0.0",
		"react-dom": "^18.0.0 || ^19.0.0"
	},
	"devDependencies": {
		"@storybook/addon-docs": "9.1.4",
		"@storybook/addon-onboarding": "9.1.4",
		"@storybook/react-vite": "9.1.4",
		"@types/graphql-deduplicator": "2.0.2",
		"@types/luxon": "3.7.1",
		"@types/papaparse": "5.3.16",
		"@types/react": "19.1.12",
		"@types/react-syntax-highlighter": "15.5.13",
		"esbuild": "0.25.9",
		"esbuild-plugin-copy": "2.1.1",
		"eslint-plugin-storybook": "9.1.4",
		"formik": "2.4.6",
		"glob": "11.0.3",
		"graphql": "16.11.0",
		"react": "19.1.1",
		"react-dom": "19.1.1",
		"storybook": "9.1.4",
		"typescript": "5.9.2",
		"vite": "7.1.4",
		"vite-plugin-graphweaver": "workspace:*",
		"vitest": "3.2.4"
	},
	"keywords": [
		"graphql",
		"gql",
		"headless",
		"cms",
		"postgres",
		"postgresql",
		"mysql",
		"sqlite"
	]
}
