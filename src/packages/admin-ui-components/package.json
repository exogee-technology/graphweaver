{
	"name": "@exogee/graphweaver-admin-ui-components",
	"version": "2.21.0",
	"description": "Components from Graphweaver's admin UI which you can use in your projects as you like",
	"license": "Apache-2.0",
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "npm run build:js && npm run build:types",
		"build:js": "node build.js",
		"build:types": "tsc --emitDeclarationOnly",
		"package:pack": "pnpm pack",
		"build:watch": "pnpm build:js --watch",
		"prettier": "prettier --write .",
		"codegen": "graphql-codegen --config ./codegen.yml",
		"test:tsc": "tsc --noEmit",
		"test:prettier": "prettier --check .",
		"version": "npm version --no-git-tag-version",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"source": "src/index.ts",
	"directories": {
		"lib": "lib"
	},
	"files": [
		"lib"
	],
	"dependencies": {
		"@apollo/client": "3.13.5",
		"jwt-decode": "4.0.0",
		"@exogee/graphweaver-apollo-client": "workspace:*",
		"@graphiql/toolkit": "0.11.3",
		"@tanstack/react-form": "1.23.8",
		"@tanstack/react-table": "8.21.3",
		"@tiptap/extension-link": "3.15.3",
		"@tiptap/react": "3.15.3",
		"@tiptap/starter-kit": "3.15.3",
		"@types/react-dom": "19.2.3",
		"clsx": "2.1.1",
		"downshift": "9.0.13",
		"graphiql": "4.1.2",
		"graphql-deduplicator": "2.0.6",
		"luxon": "3.7.2",
		"motion": "12.27.1",
		"papaparse": "5.5.3",
		"react-day-picker": "9.11.1",
		"react-error-boundary": "6.0.2",
		"react-fast-compare": "3.2.2",
		"react-hot-toast": "2.6.0",
		"react-resizable-panels": "3.0.6",
		"react-syntax-highlighter": "16.1.0",
		"tiptap-markdown": "0.9.0",
		"wouter": "3.9.0",
		"zod": "4.3.5"
	},
	"peerDependencies": {
		"formik": "^2.4.6",
		"graphql": "16",
		"react": "^18.0.0 || ^19.0.0",
		"react-dom": "^18.0.0 || ^19.0.0"
	},
	"devDependencies": {
		"@storybook/addon-docs": "10.1.11",
		"@storybook/addon-onboarding": "10.1.11",
		"@storybook/react-vite": "10.1.11",
		"@types/graphql-deduplicator": "2.0.2",
		"@types/luxon": "3.7.1",
		"@types/papaparse": "5.5.2",
		"@types/react": "19.2.7",
		"@types/react-syntax-highlighter": "15.5.13",
		"esbuild": "0.27.1",
		"esbuild-plugin-copy": "2.1.1",
		"eslint-plugin-storybook": "10.1.11",
		"formik": "2.4.6",
		"glob": "11.1.0",
		"graphql": "16.12.0",
		"react": "19.2.3",
		"react-dom": "19.2.3",
		"storybook": "10.1.11",
		"typescript": "5.9.3",
		"vite": "7.3.0",
		"vite-plugin-graphweaver": "workspace:*",
		"vitest": "4.0.18"
	},
	"keywords": [
		"graphql",
		"gql",
		"headless",
		"cms",
		"postgres",
		"postgresql",
		"mysql",
		"sqlite"
	]
}
