{
	"name": "@exogee/graphweaver-admin-ui-components",
	"version": "1.0.0-beta.1",
	"description": "Components from Graphweaver's admin UI which you can use in your projects as you like",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"build": "npm run build:js && npm run build:types",
		"build:js": "node build.js",
		"build:types": "tsc --emitDeclarationOnly",
		"package:pack": "pnpm pack",
		"build:watch": "pnpm build:js --watch",
		"prettier": "prettier --write .",
		"codegen": "graphql-codegen --config ./codegen.yml",
		"test": "vitest",
		"test:tsc": "tsc --noEmit",
		"test:prettier": "prettier --check .",
		"version": "npm version --no-git-tag-version"
	},
	"main": "lib/index.js",
	"source": "src/index.ts",
	"directories": {
		"lib": "lib"
	},
	"files": [
		"lib"
	],
	"dependencies": {
		"@apollo/client": "3.10.4",
		"@exogee/graphweaver-apollo-client": "workspace:*",
		"@graphiql/toolkit": "0.9.1",
		"clsx": "2.1.1",
		"downshift": "8.5.0",
		"graphiql": "3.2.2",
		"graphql": "16.8.1",
		"graphql-deduplicator": "2.0.6",
		"luxon": "3.4.4",
		"papaparse": "5.4.1",
		"react-data-grid": "7.0.0-beta.20",
		"react-hot-toast": "2.4.1"
	},
	"peerDependencies": {
		"formik": "2.4.6",
		"graphql": "16.8.1",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-router-dom": "6.22.3"
	},
	"devDependencies": {
		"@types/graphql-deduplicator": "2.0.2",
		"@types/luxon": "3.4.2",
		"@types/papaparse": "5.3.14",
		"@types/react": "18.3.2",
		"esbuild": "0.20.2",
		"esbuild-plugin-copy": "2.1.1",
		"esbuild-plugin-svgr": "2.1.0",
		"formik": "2.4.6",
		"glob": "10.3.12",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"typescript": "5.4.5",
		"vite": "5.2.11",
		"vite-plugin-svgr": "3.2.0",
		"vitest": "1.6.0"
	},
	"keywords": [
		"graphql",
		"gql",
		"headless",
		"cms",
		"postgres",
		"postgresql",
		"mysql",
		"sqlite"
	]
}
